<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<AnchorPane id="AnchorPane" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8" fx:controller="assignment11.InterfaceController">
   <children>
      <VBox alignment="CENTER" spacing="10.0">
         <children>
            <ImageView fx:id="weatherInfoImage" pickOnBounds="true" preserveRatio="true" />
            <Text fx:id="weatherInfoText" strokeType="OUTSIDE" strokeWidth="0.0" />
            <ChoiceBox fx:id="stationChoice" />
            <Button mnemonicParsing="false" onAction="#refreshData" text="Refresh Data" />
            <Hyperlink text="buienradar.nl" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
